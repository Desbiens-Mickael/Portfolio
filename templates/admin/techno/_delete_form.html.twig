<form method="post" action="{{ path('techno_delete', {'id': techno.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette techno ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ techno.id) }}">
    <button class="btn btn-outline-danger mt-3"><i class="bi bi-trash3"></i> supprimer</button>
</form>
